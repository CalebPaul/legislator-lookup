<ul>
  <li class="list-group-item active" >Local Legislators</li>
  {{#each model.legislators as |legislator|}}
    {{log 'age' legislator.birthday}}
    <li class="list-group-item"> {{legislator.first_name}} {{legislator.last_name}} <small>{{moment-calendar legislator.birthday}}</small><br>Contact: <a href=  {{legislator.contact_form}}>{{legislator.contact_form}}</a></li>
  {{/each}}
</ul>
